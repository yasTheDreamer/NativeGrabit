{
  "name": "NativeGrabit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "adb reverse tcp:3000 tcp:3000 & npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest --verbose --silent Test.spec.js",
    "testCoverage": "jest --verbose --silent --coverage Test.spec.js",
    "lint": "eslint .",
    "detox": "detox test --configuration android.debug"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.8.0",
    "@react-navigation/stack": "^5.10.0",
    "react": "16.13.1",
    "react-dom": "^16.14.0",
    "react-native": "0.63.3",
    "react-native-fbsdk": "^2.0.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.12.0",
    "react-native-vector-icons": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.15",
    "babel-jest": "^25.1.0",
    "detox": "^17.10.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.5.1",
    "jest": "^25.5.4",
    "jest-circus": "^26.6.1",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/setupEnzyme.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base)/"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "timers": "fake"
  }
}
